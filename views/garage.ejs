<h1> My Garage</h1>
<p> Garage index page. this will contain the garage of cars. </p>
<p><a href="/my-garage/add">Add</a></p>

<h3>My Cars</h3>

<% cars.forEach(function(car) { %>
	<a href="/my-garage/<%= car._id %>">
		<div class="card blue-grey darken-1">
			<div class="card-content white-text">
				<span><%= car.make %> <%= car.model %></span>
				<ul>
					<li>Year: <%= car.year %></li>
					<li>Mileage: <%= car.mileage %></li>
					<li>License Plate: <%= car.licensePlate %></li>
					<li>Description: <%= car.description %></li>
					<li>Available to Swap?
						<a href="/my-garage/<%= car._id %>/toggle">
              			<% if (car.isSwappable) { %>
                		<i class="fa fa-check-square-o" aria-hidden="true"></i>
              			<% } else { %>
						<i class="fa fa-square-o" aria-hidden="true"></i>
              			<% } %>
            			</a>
					</li>
				</ul>
			</div>
		</div>
	</a>
<% }) %>

<!--If current swap exists, then display:-->
<h3>Current Swap</h3>
